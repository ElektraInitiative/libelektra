Storage: dump
Mountpoint: system/tmount/kdb_ls
DiffType: File

< kdb mount $File $Mountpoint $Storage

< kdb set $Mountpoint/test bla
< kdb set $Mountpoint/test/foo/bar bur
< kdb set $Mountpoint/test/fizz/buzz fizzbuzz
< kdb set $Mountpoint/tost blu
< kdb set $Mountpoint/tost/level lvl

< kdb ls $Mountpoint
< kdb ls $Mountpoint --max-depth=1
< kdb ls $Mountpoint --min-depth=1 --max-depth=2
< kdb ls $Mountpoint/test
< kdb ls $Mountpoint -v

< kdb umount $Mountpoint
